{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "module": "esnext",
        "jsx": "preserve",
        "lib": ["dom", "esnext"],
        "incremental": true
